<launch>
	<!-- vim: set ft=xml noet : -->
	<!-- example launch script for ArduPilotMega based FCU's -->
        <arg name="fcu_url" default="/dev/ttyS1:921600" />	<!-- /dev/ttyS1:57600 -->
        <arg name="gcs_url" default="udp://:14556@192.168.2.3:14550" />	
	<arg name="tgt_system" default="1" />
	<arg name="tgt_component" default="1" />
	<arg name="log_output" default="screen" /> 
	
	<include file="$(find mavros)/launch/node.launch">
		<arg name="pluginlists_yaml" value="$(find mavros_rplidar_pid)/yaml/apm_pluginlists.yaml" />
		<arg name="config_yaml" value="$(find mavros_rplidar_pid)/yaml/apm_config.yaml" />
		<arg name="fcu_url" value="$(arg fcu_url)" />
		<arg name="gcs_url" value="$(arg gcs_url)" />
		<arg name="tgt_system" value="$(arg tgt_system)" />
		<arg name="tgt_component" value="$(arg tgt_component)" />
		<arg name="log_output" value="$(arg log_output)" />
	</include>

    <node name="offboard_rplidar_node" pkg="mavros_rplidar_pid" type="main_rplidar_pid" output="screen"/>
</launch>
